            var questionAlreadyOpen = $(this).parents('.question-container').hasClass('active');
            closeAllQuestions();
            if(!questionAlreadyOpen){
                openQuestion(this);
            }
        });
        var closeAllQuestions = function(){
            $('.question-container.active').removeClass('active').children('.answer').slideUp(300);
        };

        var openQuestion = function(question){
            $(question).siblings('.answer').slideToggle(600).parents('.question-container').toggleClass('active');
        };
    });
