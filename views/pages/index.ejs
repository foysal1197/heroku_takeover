    ], function ($, modal, customerData) {
        $(function () {
            $('#newsletter-validate-detail').on('submit', function (event) {
                event.preventDefault();

                if ($('#newsletter-validate-detail').valid()) {
                    $("body").trigger('processStart');
                    var subscribeEmail = $('#newsletter').val();
                    var ajaxUrl = "https://www.kipling.com/int/rest/V1/vf-newsletter/subscribe/" + subscribeEmail;
                    $.ajax({
                        url: ajaxUrl,
                        type: 'GET',
                    }).done(function (response) {
                        $("body").trigger('processStop');
                        var responseContent = JSON.parse(response)
                        if (responseContent.success === true) {
                            var options = {
                                type: 'popup',
                                parentModalClass: '_has-modal newletter-popup-container',
                                responsive: true,
                                innerScroll: true,
                                buttons: [{

                                }]
                            };
                            var popup = modal(options, $('#popup-modal-newsletter'));
                            $("#popup-modal-newsletter").modal("openModal");
                            var sections = ['gtm'];
                            customerData.invalidate(sections);
                            customerData.reload(sections, true);
                        } else {
                            $('.newsletter-error').html(responseContent.error);
                        }
                    }).fail(function (response) {
                        $('.newsletter-error').html($.mage.__('Something went wrong with the subscription.'));
                    });
                }
            });
        });
    });</script>
